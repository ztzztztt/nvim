{
    "header": {
        "prefix": "header",
        "body": [
            "#!/usr/bin/env python",
            "# -*- encoding: utf-8 -*-",
            "\"\"\"",
            "@Author  : zhoutao",
            "@License : (C) Copyright 2013-2021, China University of Petroleum",
            "@Contact : zhoutao@s.upc.edu.cn",
            "@Software: neovim",
            "@File    : $TM_FILENAME_BASE",
            "@Time    : $CURRENT_YEAR/$CURRENT_MONTH/$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
            "@Desc    : ",
            "\"\"\"",
            "$0"
        ],
        "description": "Add file header for python file"
    },
    "main": {
        "prefix": "ifm",
        "body": [
            "if __name__ == \"__main__\"",
            "   pass",
            "$0"
        ]
    },
    "doc": {
        "prefix": "doc",
        "body": [
            "\"\"\"",
            "${1:docstring}",
            ":param ${2:params}:",
            ":return:",
            "\"\"\""
        ],
        "description": "Add function docstring for python 3"
    },
    "dof": {
        "prefix": "dof",
        "body": [
            "def ${1:fname}(${2:args}):",
            "\t\"\"\"",
            "\t${3:docstring}",
            "\t:param ${2:params}:",
            "\t:return:",
            "\t\"\"\"",
            "\tpass"
        ],
        "description": "Add function with docstring for python3"
    }
}